{
  "extends": [
    "@nuxt/eslint-config",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "off",
    "vue/require-default-prop": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ]
  }
}